    "npm-check-updates": "^12.5.11",    
    "npm-run-all": "^4.1.5",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
